<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    
      
<% if (name) { %>
  <p>Welcome <%= name %>!<%= id %></p>
  <p><%= notes %></p>
<% } %>
<form action="/submit" method="POST">
  <div class="form-group">
    <input type="text" class="form-control text-center" name="title" placeholder="What's your title?" required>
  </div>
  <div class="form-group">
    <input type="text" class="form-control text-center" name="content" placeholder="What's your content?" required>
  </div>
  <button type="submit" class="btn btn-dark">Submit</button>
</form>
    <hr>
    <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
  </div>
</div>

<%- include('partials/footer') %>
